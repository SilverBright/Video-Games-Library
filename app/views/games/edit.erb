<h1>EDIT GAME PAGE</h1>

<form action="/games/<%= @game.id %>" method='post'>
  <input id='hidden' type='hidden' name='_method' value='patch'>
   Title:<br>
  <input type="text" name="titles"  value='<%= @game.titles %>'><br>
   Platform:<br>
  <input type="text" name="platform" value='<%= @game.platform %>'><br>
    
  <br></br>


  <input type='submit' id='submit' value='Submit Changes'>
</form>
